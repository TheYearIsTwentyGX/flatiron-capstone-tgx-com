<p id="notice"><%= notice %></p>

<h1>Facility Accesses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @facility_accesses.each do |facility_access| %>
      <tr>
        <td><%= link_to 'Show', facility_access %></td>
        <td><%= link_to 'Edit', edit_facility_access_path(facility_access) %></td>
        <td><%= link_to 'Destroy', facility_access, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Facility Access', new_facility_access_path %>
